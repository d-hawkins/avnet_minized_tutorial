# -----------------------------------------------------------------------------
# Targets
# -----------------------------------------------------------------------------
#
default: apps

# -----------------------------------------------------------------------------
# Build parameters
# -----------------------------------------------------------------------------
#
# Compiler
CC = gcc

# Compiler options
CPPFLAGS += -Wall

# -----------------------------------------------------------------------------
# Applications
# -----------------------------------------------------------------------------

APPS = uboot_crc32

apps: $(APPS)

clean:
	@rm -rf $(APPS) *.o
